


<div class="booyah-box col-10 offset-1">
  <h1>Enter Your Comment Details</h1>
  <%= simple_form_for @comment do |f| %>
    <%= f.input :name %>
    <%= f.input :productname %>
    <%= f.input :description %>
    <br />
    <%= f.submit 'Create', class: 'btn btn-primary' %>
  <% end %>
</div>